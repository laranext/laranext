{
    "name": "laranext/{{ name }}",
    "description": "A wonderful administration interface for Laravel.",
    "keywords": [
        "laravel",
        "admin",
        "modular",
        "laranext",
        "packages"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Ahsan Abrar",
            "email": "iahsanabrar@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "{{ namespace }}\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
